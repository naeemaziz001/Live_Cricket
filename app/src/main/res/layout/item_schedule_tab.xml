<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent" android:layout_height="fill_parent" android:layout_margin="7dp"
    android:background="@drawable/shape_obj"

  xmlns:android="http://schemas.android.com/apk/res/android">

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="5dip"
        >


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/aus"
                android:id="@+id/imageView1"
                android:layout_gravity="center"
                android:layout_weight="1" />


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/vs"
                android:layout_marginTop="9dp"
                android:layout_gravity="center"
                android:visibility="invisible"
                android:id="@+id/imageView2"
                android:layout_weight="1" />


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/pak"
            android:layout_gravity="center"
            android:id="@+id/imageView3"
            android:layout_weight="1" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#7B922A"

        android:padding="5dip"
        android:foregroundTint="@android:color/holo_blue_bright">

        <TextView
            android:text="AUS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:id="@+id/textView1"
            android:textStyle="bold"
            android:textSize="20dp"
            android:layout_weight="2"
            android:elevation="2dp"
            android:lines="1"
            />
        <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:id="@+id/vb"
            android:textStyle="bold"
            android:textSize="12dp"
            android:layout_weight="1"

            />
        <TextView
            android:text="PAK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:id="@+id/textView2"
            android:textStyle="bold"
            android:textSize="20dp"
            android:layout_weight="2"

            />
    </TableRow>
    <!--android:background="#005a27"-->
    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:background="@drawable/shape_sch"
        android:padding="5dip"
        android:showDividers="beginning|middle|end"
        android:visibility="visible">

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:id="@+id/textView3"
            android:textColor="#FCFF00"
            android:layout_weight="1"/>

        <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:id="@+id/textView4"
        android:layout_weight="1" android:textColor="#FCFF00" />
        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:id="@+id/textView5"
            android:textColor="#FCFF00"
            android:layout_weight="1"

            />
    </TableRow>

</TableLayout >